{
  "name": "vue3-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --max-warnings=0 --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location .eslintcache",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@breeze/eslint-config": "workspace:*",
    "@vue/eslint-config-typescript": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@tsconfig/node24": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/tsconfig": "catalog:",
    "npm-run-all2": "catalog:",
    "prettier": "catalog:",
    "sass": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "unplugin-auto-import": "catalog:",
    "vite": "catalog:",
    "vite-plugin-vue-devtools": "catalog:",
    "vue-tsc": "catalog:"
  }
}
